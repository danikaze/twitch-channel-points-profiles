{
  "manifest_version": 2,

  "name": "Chrome Extension Boilerplate",
  "short_name": "Boilerplate",
  "description": "Boilerplate to create Chrome extensions using TypeScript + ReactJS",
  "version": "AUTO_FILLED",

  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png"
  },

  "browser_action": {
    "default_title": "",
    "default_icon": "icons/icon128.png",
    "default_popup": "index.html"
  },
  "permissions": ["http://*/*", "https://*/*"]
}
